{
  "cli": {
    "version": ">= 3.17.1"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development"
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview"
    },
    "production": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "channel": "production"
    }
  },
  "submit": {
    "production": {}
  },
  "plugins": [
    [
      "expo-navigation-bar",
      {
        "position": "absolute",
        "visibility": "hidden",
        "behavior": "inset-swipe",
        "backgroundColor": "#00000000"
      }
    ]
  ]
}